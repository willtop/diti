    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--config_path", "../config/celeba128/diti64.yml",
                     "--run_path", "../runs",
                     "--method", "diti",
                     "-n", "1",
                     "-g", "1",
                     "-nr", "0"],
            "env": {"PYTHONPATH": "${workspaceFolder}"},
            "cwd": "${fileDirname}"
        }
    ]
}